      </router-link>\n        <div class=\"indent-right\">\n          <a href=\"\" v-if=\"item.title !== item.original_title\">{{item.title}}/{{item.original_title}}</a>\n          <a href=\"\" v-else>{{item.title}}</a>\n          <br>\n          <span\n            v-for=\"(pubdate, index) in item.pubdates\"\n            :key=\"'pub'+index\"\n          >\n            {{pubdate}}/\n          </span>\n          <span\n            v-for=\"(director, index) in item.directors\"\n            :key=\"'direc'+index\"\n          >\n            {{director.name}}/\n          </span>\n          <span\n            v-for=\"(cast, index) in item.casts\"\n            :key=\"'cast'+index\"\n          >\n            {{cast.name}}/\n          </span>\n          <span\n            v-for=\"(genre, index) in item.genres\"\n            :key=\"'genre'+index\"\n          >\n            {{genre}}/\n          </span>\n          <Rate :rating=\"item.rating.average|calc2\" />\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Rate from '@/components/Rate.vue'\nimport { getNewMovies, testsearch } from '@/network/request'\n\nexport default {\n  name: 'chart',\n  components: { Rate },\n  data () {\n    return {\n      newmovielist: [],\n      loading: false\n    }\n  },\n  filters: {\n    calc2: function (value) {\n      return value / 2\n    }\n  },\n  created () {\n    getNewMovies().then(res => {\n      this.newmovielist = res.data.subjects\n      this.loading = false\n    })\n    testsearch().then(res => {\n      console.log('this is test')\n      console.log(res)\n    })\n  },\n  mounted () {\n    this.loading = false\n  }\n}\n</script>\n\n<style scoped lang=\"less\" rel=\"stylesheet/less\">\n.chart {\n  width: 675px;\n  margin-left: 135px;\n  h1 {\n    color: #494949;\n    font-size: 26px;\n    margin-top: 30px;\n  }\n  h2 {\n    font-size: 16px;\n    color: #007722;\n    margin-top: 10px;\n  }\n  .hr {\n    width: 100%;\n    height: 20px;\n    border-bottom: 1px dashed #dddddd;\n  }\n  .indent {\n    display: flex;\n    flex-direction: row;\n    padding: 10px 0 10px 0;\n    .img-contain {\n      width: 75px;\n      height: 112px;\n      overflow: hidden;\n      img {\n        width: 100%;\n      }\n      img:hover {\n        transform: scale(1.5, 1.5);\n        transition: all 2s;\n      }\n    }\n    .indent-right {\n      margin-left: 25px;\n      a {\n        font-size: 14px;\n        color: #3377aa;\n        text-decoration: none;\n      }\n      span {\n        color: #666666;\n        font-size: 13px;\n      }\n    }\n  }\n}\n</style>\n"]}]}